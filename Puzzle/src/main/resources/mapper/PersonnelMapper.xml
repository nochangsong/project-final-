<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	<mapper namespace="P_Card">
	<insert id="insertCard" parameterType="model.PersonnelCommand">
		insert into PersonnelCard 
		values (#{Name},#{Email},#{Age},#{Position_Name},#{HireDate},#{RetireDate},#{RetireType},#{Dept_Num},#{Work_Num},#{Position_Num},#{PhoneNum},#{right})
	</insert>
	</mapper>